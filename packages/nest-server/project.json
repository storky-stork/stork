{
  "root": "packages/nest-server",
  "sourceRoot": "packages/nest-server/src",
  "projectType": "application",
  "targets": {
    "deploy": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx build nest-server",
          "docker build -f ./packages/nest-server/Dockerfile . -t api"
        ],
        "parallel": false
      }
    },
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/nest-server",
        "main": "packages/nest-server/src/main.ts",
        "tsConfig": "packages/nest-server/tsconfig.app.json",
        "assets": [
          "packages/nest-server/src/assets"
        ],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "packages/nest-server/src/environments/environment.ts",
              "with": "packages/nest-server/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "nest-server:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/nest-server/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/nest-server"
      ],
      "options": {
        "jestConfig": "packages/nest-server/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}